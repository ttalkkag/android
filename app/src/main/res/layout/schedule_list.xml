<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="200dp">

    <View
        android:id="@+id/view_list_sch_bg"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/shape_rect_10_white000_fill_b3b3b3_line"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_list_sch_circle"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/shape_circle_3e8bff_fill"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="제목:"
        android:textStyle="bold"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.14" />

    <TextView
        android:id="@+id/tv_list_sch_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="제목 입력"
        android:textStyle="bold"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        android:layout_marginStart="65dp"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.14" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="날짜:"
        android:textStyle="bold"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.32" />

    <TextView
        android:id="@+id/tv_list_sch_date"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="날짜 입력"
        android:textStyle="bold"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        android:layout_marginStart="65dp"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.32" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="시간:"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/tv_list_sch_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="시간 입력"
        android:textStyle="bold"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        android:layout_marginStart="65dp"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="내용:"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.68" />

    <TextView
        android:id="@+id/tv_list_sch_detail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="내용 입력"
        android:textStyle="bold"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        android:layout_marginStart="65dp"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.68" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="장소:"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.86" />

    <TextView
        android:id="@+id/tv_list_sch_location"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="65dp"
        android:text="장소 입력"
        android:textAlignment="textStart"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view_list_sch_bg"
        app:layout_constraintEnd_toEndOf="@id/view_list_sch_bg"
        app:layout_constraintStart_toStartOf="@+id/view_list_sch_bg"
        app:layout_constraintTop_toTopOf="@+id/view_list_sch_bg"
        app:layout_constraintVertical_bias="0.86" />


</androidx.constraintlayout.widget.ConstraintLayout>